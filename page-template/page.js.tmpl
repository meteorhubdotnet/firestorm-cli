import { Router } from 'meteor/meteorhubdotnet:iron-router';

Router.route(
	'PAGE_PATH_GOES_HERE',
	{
		// Route name
		name: 'TEMPLATE_NAME_GOES_HERE',

		// Page Data
		data: function() {

			// Page title to display in top navbar
			const title = 'PAGE_TITLE_GOES_HERE';

			// Page params
			const params = this.params;

            // Return
			return { params, title };

		}

	},

);
